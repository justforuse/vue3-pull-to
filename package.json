{
  "name": "vue3-pull-to",
  "version": "0.1.0",
  "description": "Vue 3 pull down to refresh",
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:site": "pnpm run build:lib && vite build",
    "build:lib": "vite build -c vite.config.lib.js",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justforuse/vue3-pull-to.git"
  },
  "author": "justforuse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justforuse/vue3-pull-to/issues"
  },
  "homepage": "https://github.com/justforuse/vue3-pull-to#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "sass": "^1.79.5",
    "vite": "^5.4.8"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
